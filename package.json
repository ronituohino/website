{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.development -- pnpm dlx prisma migrate dev && NODE_ENV=development next dev",
    "build": "NODE_ENV=production next build",
    "start": "NODE_ENV=production next start",
    "lint": "next lint",
    "test": "dotenv -e .env.test -- pnpm dlx prisma db push --force-reset && pnpm dlx playwright test",
    "test-server": "NODE_ENV=test next dev",
    "docker-build": "DOCKER_BUILDKIT=1 docker build -t nextjs-docker .",
    "docker-run": "docker run -p 3000:3000 nextjs-docker"
  },
  "dependencies": {
    "@next/font": "13.1.1",
    "@prisma/client": "^4.8.1",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "axios": "^1.2.2",
    "eslint": "8.31.0",
    "eslint-config-next": "13.1.1",
    "next": "13.1.1",
    "panzoom": "^9.4.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-zoom-pan-pinch": "^3.0.6",
    "swr": "^2.0.0",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.32.2",
    "prisma": "^4.8.1"
  }
}
